@font-face {
     font-family: 'OpenSans-SemiBold';
     src: url('OpenSans-SemiBold.eot') format('eot'); /* IE9 Compat Modes */
     src: url('OpenSans-SemiBold.woff2') format('woff2'), /* Super Modern Browsers */
          url('OpenSans-SemiBold.woff') format('woff'), /* Pretty Modern Browsers */
          url('OpenSans-SemiBold.ttf')  format('truetype'), /* Safari, Android, iOS */
          url('OpenSans-SemiBold.otf') format("opentype");
   }
